{
    "name": "Data 8: project3",
    "build": {
        "dockerfile": "../dockerfile",
        "context": "../.."
    },
    "waitFor": "onCreateCommand",
    "customizations": {
        "codespaces": {
            "openFiles": []
        },
        "vscode": {
            "settings": {
                "python.defaultInterpreterPath": "/usr/local/bin/python",
                "workbench.editorAssociations": {
                    "*.ipynb": "jupyter-notebook"
                },
                "notebook.compactView": false
            },
            "extensions": [
                "ms-python.python",
                "ms-toolsai.jupyter",
                "MS-vsliveshare.vsliveshare"
            ]
        }
    },
    "postAttachCommand": "code /workspaces/materials-fa22/materials/fa22/project/project3/project3.ipynb"
}