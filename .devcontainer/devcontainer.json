{
	"name": "Data 8 notebooks",
	"build": {
		"dockerfile": "dockerfile",
		"context": ".."
	},
	"customizations": {
		"codespaces": {
			"openFiles": []
		},
		"vscode": {
			"settings": {
				"python.defaultInterpreterPath": "/usr/local/bin/python",
				"workbench.editorAssociations": {
					"*.ipynb": "jupyter-notebook"
				},
				"notebook.compactView": false
			},
			"extensions": [
				"ms-python.python",
				"ms-toolsai.jupyter",
				"MS-vsliveshare.vsliveshare"
			]
		}
	}
}
